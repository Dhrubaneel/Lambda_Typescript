{
  "name": "lambda-typescript",
  "version": "1.0.0",
  "description": "This is a aws lambda function build in Typescript",
  "main": "lambda.js",
  "scripts": {
    "test": "tsc && mocha -r ts-node/register test/**/*.ts",
    "build:doc": "jsdoc2md --no-cache --files src/**/*.ts --configure jsdoc2md.json --template README.hbs > README.md && doctoc README.md"
  },
  "author": "Dhrubaneel Das",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "@types/aws-lambda": "0.0.31",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "aws-lambda": "^0.1.2",
    "chai": "^4.1.2",
    "doctoc": "^1.3.1",
    "jsdoc-babel": "^0.4.0-alpha.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.9.2"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {}
}
